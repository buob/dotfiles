[user]
	name = Jake Buob
	email = jake@mojotech.com
[core]
	editor = emacs
	excludesfile = /Users/jakebuob/.gitignore_global
[merge]
	tool = vimdiff
[mergetool]
	keepBackup = true
[color]
	ui = true
[push]
	default = current
[alias]
	tree = log --graph --all --decorate --pretty=oneline --abbrev-commit
[rebase]
  autosquash = true
  autostash = true
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
